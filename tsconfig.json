{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "components": [
        "src/components/*"
      ],
      "lib": [
        "src/lib/*"
      ],
      "pages": [
        "src/pages/*"
      ],
      "styles": [
        "src/style/*s"
      ],
      "types": [
        "src/types/*"
      ]
    }
  },
  "include": [
    "src"
  ],
}
